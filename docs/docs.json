{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Weblinq API",
  "colors": {
    "primary": "#c46449",
    "light": "#d4795c",
    "dark": "#a35038"
  },
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude"]
  },
  "favicon": "/favicon.svg",
  "icons": {
    "library": "lucide"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Introduction",
            "pages": ["index", "quickstart", "authentication"]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/examples",
              {
                "group": "Use Cases",
                "pages": ["guides/e-commerce", "guides/news-extraction", "guides/social-media"]
              },
              {
                "group": "Best Practices",
                "pages": ["guides/security", "guides/rate-limiting", "guides/error-handling"]
              }
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/overview",
              "api-reference/success",
              "api-reference/errors",
              "api-reference/rate-limits"
            ]
          }
        ],
        "openapi": {
          "source": "https://api.weblinq.dev/doc",
          "directory": "api-reference"
        }
      },
      {
        "tab": "Resources",
        "groups": [
          {
            "group": "Support",
            "pages": ["support/faq", "support/contact"]
          },
          {
            "group": "Legal",
            "pages": ["legal/terms", "legal/privacy-policy"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/devhims/weblinq",
          "icon": "github"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@weblinq.dev",
          "icon": "mail"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/logo.png",
    "dark": "/logo/logo.png"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://weblinq.dev/dashboard/api-keys"
    }
  },
  "feedback": {
    "suggestEdit": true,
    "raiseIssue": true
  },
  "search": {
    "prompt": "Search the docs..."
  },

  "footer": {
    "socials": {
      "github": "https://github.com/devhims",
      "twitter": "https://x.com/devhims"
    }
  }
}
